			"new url: https://www.doc.ic.ac.uk/~ahanda/VaFRIC/test_datasets.html",
			"paper: https://www.doc.ic.ac.uk/~ahanda/VaFRIC/eccv2012.pdf",
			"implementation: https://github.com/Rintarooo/OpenDTAM",
			"curl -O https://www.doc.ic.ac.uk/~ahanda/HighFrameRateTracking/traj_30_seconds.tgz"
		]
	},
	"img_size":{
		"width_init": 640.0,
		"height_init": 480.0,
		"width": 640,
		"height": 480
	},
	"K":{
		"fx_init":481.2,
		"fy_init":-480.0,
		"cx_init":319.5,
		"cy_init":239.5
	},
	"costvolume": {
		"depth_min":0.5,
		"depth_max":10.0,
